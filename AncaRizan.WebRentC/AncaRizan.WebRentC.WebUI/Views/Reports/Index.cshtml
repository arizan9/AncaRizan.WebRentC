<h4>Gold Customers</h4>

@foreach (var item in Model.GoldCustomers)
{
    <dl class="dl-horizontal">

        <dt>ID:</dt>
        <dd>@item.CostumerID</dd>

        <dt>FirstName</dt>
        <dd>@item.FirstName</dd>

        <dt>LastName</dt>
        <dd>@item.LastName</dd>

        <dt>BirthDate</dt>
        <dd>@item.BirthDate</dd>

    </dl>
}

