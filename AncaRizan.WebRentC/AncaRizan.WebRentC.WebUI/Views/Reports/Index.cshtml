@model Anca.Rizan.WebRentC.Core.ViewModels.ReportsViewModel

@{
    ViewBag.Title = "Index";
}

<h4>Most Rented Car</h4>

<dl class="dl-horizontal">

    <dt>
        @Html.DisplayNameFor(model => model.MostRentedCar.Plate)
    </dt>

    <dd>
        @Html.DisplayFor(model => model.MostRentedCar.Plate)
    </dd>

    <dt>
        @Html.DisplayNameFor(model => model.MostRentedCar.Manufacturer)
    </dt>

