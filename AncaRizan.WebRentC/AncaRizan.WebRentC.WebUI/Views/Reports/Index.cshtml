        @Html.DisplayFor(model => model.MostRentedCar.Manufacturer)
    </dd>

    <dt>
        @Html.DisplayNameFor(model => model.MostRentedCar.Model)
    </dt>

    <dd>
        @Html.DisplayFor(model => model.MostRentedCar.Model)
    </dd>

    <dt>
        @Html.DisplayNameFor(model => model.MostRentedCar.PricePerDay)
    </dt>

    <dd>
        @Html.DisplayFor(model => model.MostRentedCar.PricePerDay)
    </dd>

</dl>

